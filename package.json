{
  "name": "qtask2",
  "version": "0.4.0",
  "author": "Steve Simkins",
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "bun-types": "1.3.1",
    "turbo": "^2.5.8"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "description": "A monorepo template built with Bun, Hono, Vite, and React",
  "homepage": "https://github.com/stevedylandev/bhvr",
  "keywords": [
    "bun",
    "hono",
    "react",
    "vite",
    "monorepo",
    "turbo"
  ],
  "license": "MIT",
  "packageManager": "bun@1.2.4",
  "scripts": {
    "dev": "turbo dev",
    "dev:client": "turbo dev --filter=client",
    "dev:server": "turbo dev --filter=server",
    "build": "turbo build",
    "build:client": "turbo build --filter=client",
    "build:server": "turbo build --filter=server",
    "lint": "biome lint .",
    "type-check": "turbo type-check",
    "test": "turbo test",
    "postinstall": "turbo build --filter=server",
    "format": "biome format . --write"
  },
  "workspaces": [
    "./server",
    "./client"
  ],
  "dependencies": {
    "@trpc/client": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@trpc/tanstack-react-query": "^11.7.1",
    "superjson": "^2.2.5",
    "zod": "^4.1.12"
  }
}
